{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "func-names": [
      "error",
      "never"
    ],
    "brace-style": [
      "error",
      "stroustrup"
    ],
    "no-underscore-dangle": 0,
    "consistent-return": 0,
    "array-callback-return": 0
  },
  "plugins": [
    "react"
  ]
}